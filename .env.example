# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api
NODE_ENV=development

# Production Password Protection
AUTH_PASSWORD=your-secure-password-here

# Database
DATABASE_URL=postgresql://antevus:antevus_dev_password@localhost:5432/antevus_db
TIMESCALE_URL=postgresql://antevus:antevus_timescale_password@localhost:5433/antevus_telemetry

# Redis
REDIS_URL=redis://localhost:6379

# Authentication (Auth0)
AUTH0_SECRET=your-auth0-secret-here
AUTH0_BASE_URL=http://localhost:3000
AUTH0_ISSUER_BASE_URL=https://your-tenant.auth0.com
AUTH0_CLIENT_ID=your-client-id
AUTH0_CLIENT_SECRET=your-client-secret

# Sentry Error Tracking
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn-here
SENTRY_ORG=your-sentry-org
SENTRY_PROJECT=antevus
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# PostHog Analytics
NEXT_PUBLIC_POSTHOG_KEY=your-posthog-key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Vercel Deployment
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-vercel-org-id
VERCEL_PROJECT_ID=your-vercel-project-id

# AWS S3 (for file storage)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-west-2
S3_BUCKET_NAME=antevus-data

# API Keys
API_SECRET_KEY=generate-a-secure-random-key
WEBHOOK_SECRET=generate-a-webhook-secret

# Feature Flags
ENABLE_MOCK_DATA=true
ENABLE_DEBUG_MODE=false